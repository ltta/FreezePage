{
    "background": {"scripts": ["background.js"]},
    "browser_action": {
        "default_icon": "icon-128.png",
        "default_title": "Freeze Page - Stop Javascript"
     },
     "name": "Freeze Page",
     "description": "Turn web pages into static pages, disabling any background Javascript (JS).",
     "homepage_url": "https://github.com/ltta",
     "icons": {
         "16": "icon-16.png",
         "48": "icon-48.png",
         "128": "icon-128.png" },
     "permissions": [
         "tabs",
         "http://*/*",
         "https://*/*"
     ],
     "version": "0.1",
     "manifest_version": 2
}
